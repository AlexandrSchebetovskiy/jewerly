{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi":"off",
    "arrow-parens":"off",
    "comma-dangle":"off",
    "require-jsdoc":"off",
    "linebreak-style":"off",
    "operator-linebreak":"off",
    "no-unused-vars":"warn",
    "spaced-comment":"off",
    "quotes":"warn",
    "guard-for-in":"off"
        
  },
  "extends": ["eslint:recommended", "google"]
}